guest_list.insert(0, 'pandu')
guest_list.insert(2, 'prasad')
guest_list.append('srinu')
guest_list.sort()
print(guest_list)
removed_guest = guest_list.pop(0)
message = f"sorry {removed_guest} for sudden change in plan."
print(message)
print(guest_list)
removed_guest = guest_list.pop(3)
message = f"sorry {removed_guest} for sudden change in plan."
print(message)
